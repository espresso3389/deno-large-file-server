{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "program": "src/index.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "--allow-env",
        "--allow-net",
        "--allow-write",
        "--allow-read",
        "--allow-run"
      ],
      "attachSimplePort": 9229
    }
  ]
}